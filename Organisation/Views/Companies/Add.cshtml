@{
}
@model Organisation.Models.CompanyViewModel

<h1>Add Details</h1>
<form method="post">
    <div class="mt-3">
     <label class="form-label">Name</label>
     <input type="text" class="form-control" placeholder="Enter Name" asp-for="Name"/>
    </div>

   <div class="mt-3">
        <label class="form-label">Department</label>
        <select class="form-control" asp-for="Department">
            <option value="IT">IT</option>
            <option value="CSE">CSE</option>
            <option value="STEAL">STEAL</option>
        </select>
    </div>


    <div class="mt-3">
        <label class="form-label">Establish</label>
        <input type="date" class="form-control"  asp-for="Establish" />
    </div>

    <div class="mt-3">
        <label class="form-label">Address</label>
        <input type="text" class="form-control" placeholder="Enter Address" asp-for="Address" />
    </div>

    <div class="mt-3">
        <button type="Submit" class="btn btn-primary">Add</button>
    </div>
  

</form>
